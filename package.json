    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.49.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^2.8.8",
